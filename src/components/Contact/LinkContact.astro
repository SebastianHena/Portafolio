---
const {svg, url} = Astro.props
---
    

    <a
        href={url}
        target="_blank"
        class="flex flex-row items-center justify-center p-2 text-base font-bold bg-gray-50 hover:bg-gray-100 group hover:shadow dark:bg-gray-600 dark:hover:bg-gray-500 dark:text-white"
    >
    <img src={svg} alt="">
    </a>